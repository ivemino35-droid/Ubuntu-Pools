y (created_by) references auth.users(id) on delete set null,\n  constraint fk_signer foreign key (signed_by) references auth.users(id) on delete set null\n);\n\ncreate index on pool_constitutions (pool_id);\ncreate index on pool_constitutions (status);\ncreate index on pool_constitutions using gin (audit trail);"}]}]}]}]},{"name":"Constitution Template Card","classes":["gjs-t-border","constitution-template-card"],"components":[{"name":"Constitution Template Header","classes":["constitution-template-header"],"components":[{"name":"Constitution Template Title Row","classes":["constitution-template-title-row"],"components":[{"type":"image","name":"Document Icon","resizable":{"ratioDefault":1},"classes":["document-icon"],"attributes":{"alt":"File icon","src":"https://api.iconify.design/lucide-file-text.svg?color=%230f172a"}},{"tagName":"h3","type":"heading","name":"Constitution Template Title","classes":["constitution-template-title"],"components":[{"type":"textnode","content":"Template: TrustPools Constitution (ZA)"}]}]},{"tagName":"span","type":"text","name":"Constitution Version Label","classes":["constitution-version-label"],"components":[{"type":"textnode","content":"Version 1.0"}]}]},{"name":"Constitution Template Body","classes":["constitution-template-body"],"components":[{"name":"Constitution Section Block","classes":["constitution-section-block"],"components":[{"tagName":"h4","type":"heading","name":"Constitution Section Title","classes":["constitution-section-title"],"components":[{"type":"textnode","content":"1. Name & Purpose"}]},{"tagName":"p","type":"text","name":"Constitution Section Paragraph","classes":["constitution-section-paragraph"],"components":[{"type":"textnode","content":"This collective savings pool (the Pool) is established under South African law to promote mutual savings, financial inclusion, and community support among its members."}]}]},{"name":"Constitution Section Block","classes":["constitution-section-block"],"components":[{"tagName":